{
  "name": "laplacenoma",
  "type": "module",
  "version": "0.0.1",
  "packageManager": "pnpm@10.16.0",
  "description": "Reactivity analyzer for signal-based library",
  "author": "KazariEX",
  "license": "MIT",
  "repository": "KazariEX/laplacenoma",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./rules/vue": {
      "import": "./dist/rules/vue.js",
      "require": "./dist/rules/vue.cjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.cts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown -w",
    "prepack": "pnpm run build",
    "release": "bumpp --no-push -c \"release: v%s\"",
    "test": "vitest"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "@zinkawaii/eslint-config": "catalog:",
    "@zinkawaii/tsconfig": "catalog:",
    "bumpp": "catalog:",
    "eslint": "catalog:",
    "tsdown": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  }
}
